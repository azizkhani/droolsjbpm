<knowledgebase-request>
  <queries>
    <query-type>
      <queryName>Get all inserted facts</queryName>
      <factNames>
        <string>insf</string>
      </factNames>
    </query-type>
    <query-type>
      <queryName>Get named inserted fact</queryName>
      <factNames>
        <string>ninsf</string>
      </factNames>
      <args>
        <string>three</string>
      </args>
    </query-type>
  </queries>
  <globals>
    <named-fact>
      <id>prs</id>
      <fact class="org.drools.Person">
        <name>Jo</name>
      </fact>
    </named-fact>
  </globals>
  <inOutFacts>
    <named-fact>
      <id>result</id>
      <fact class="something.Result"/>
    </named-fact>
  </inOutFacts>
  <inFacts>
    <anon-fact>
      <fact class="something.Car">
        <name>saab</name>
        <type>wagon</type>
      </fact>
    </anon-fact>
  </inFacts>
</knowledgebase-request>