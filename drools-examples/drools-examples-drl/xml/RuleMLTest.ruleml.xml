                                                        <Rel>isA</Rel>
                                                    </op>
                                                    <slot>
                                                        <Ind>class</Ind>
                                                        <Var/>
                                                    </slot>
                                                    <Ind type="java.lang.Class">org.drools.chance.Message</Ind>
                                                </Atom>
                                                <Atom>
                                                    <op>
                                                        <Rel>&gt;</Rel>
                                                    </op>
                                                    <slot>
                                                        <Ind>status</Ind>
                                                        <Var>RuleId.Message.$sjoin</Var>
