                                        <oid>
                                            <Var>RuleId.$m</Var>
                                        </oid>
                                        <Rel>Message</Rel>
                                        <Reify>
                                            <And>
                                                <Atom>
                                                    <oid>
                                                        <Data>idCls</Data>
                                                    </oid>
                                                    <!--unsupported attribute - 
<cut/>
-->
                                                    <!--unsupported attribute - 
<kind>args:z</kind>
-->
                                                    <!--
<degree>
    <Data>0.1</Data>
</degree>
-->
