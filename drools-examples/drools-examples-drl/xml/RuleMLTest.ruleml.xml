                                                            <Rel>==</Rel>
                                                        </op>
                                                        <slot>
                                                            <Ind>status</Ind>
                                                            <Var>RuleId.$m.$s</Var>
                                                        </slot>
                                                        <Ind type="java.lang.Number">0</Ind>
                                                    </Atom>
                                                    <And>
                                                        <Atom>
                                                            <oid>
                                                                <Data>idFreeOp</Data>
                                                            </oid>
                                                            <!--unsupported attribute - 
<cut/>
-->
                                                            <!--unsupported attribute - 
<kind>kind:a</kind>
-->
                                                            <!--
<degree>
    <Data>0.4</Data>
</degree>
-->
                                                            <op>
                                                                <Rel>==</Rel>
                                                            </op>
                                                            <slot>
                                                                <Ind>message</Ind>
                                                                <Var/>
                                                            </slot>
                                                            <Ind type="java.lang.String">Hello</Ind>
                                                        </Atom>
                                                        <Atom>
                                                            <op>
                                                                <Rel/>
                                                            </op>
                                                            <slot>
                                                                <Ind>message</Ind>
                                                                <Var>RuleId.$m.$msg</Var>
                                                            </slot>
                                                        </Atom>
                                                    </And>
                                                </Or>
                                            </And>
                                        </Reify>
                                    </Atom>
                                    <Atom>
                                        <oid>
                                            <Var>RuleId.Message</Var>
                                        </oid>
                                        <Rel>Message</Rel>
                                        <Reify>
                                            <And>
