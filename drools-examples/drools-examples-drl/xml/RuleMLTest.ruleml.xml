                                                        <Rel>isA</Rel>
                                                    </op>
                                                    <slot>
                                                        <Ind>class</Ind>
                                                        <Var/>
                                                    </slot>
                                                    <Ind type="java.lang.Class">org.drools.chance.Message</Ind>
                                                </Atom>
                                                <Or>
                                                    <Atom>
                                                        <oid>
                                                            <Data>idBndOp</Data>
                                                        </oid>
                                                        <!--unsupported attribute - 
<cut/>
-->
                                                        <!--unsupported attribute - 
<kind>args:tolerance(0.1)</kind>
-->
                                                        <!--
<degree>
    <Data>0.4</Data>
</degree>
-->
