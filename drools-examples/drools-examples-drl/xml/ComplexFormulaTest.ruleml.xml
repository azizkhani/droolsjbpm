                                                    <op>
                                                        <Rel>==</Rel>
                                                    </op>
                                                    <slot>
                                                        <Ind>status</Ind>
                                                        <Var/>
                                                    </slot>
                                                    <Ind type="java.lang.Number">0</Ind>
                                                </Atom>
                                                <Atom>
                                                    <op>
                                                        <Rel>==</Rel>
                                                    </op>
                                                    <slot>
                                                        <Ind>status</Ind>
                                                        <Var/>
                                                    </slot>
                                                    <Ind type="java.lang.Number">1</Ind>
                                                </Atom>
                                            </And>
                                        </Reify>
                                    </Atom>
                                    <Atom>
                                        <oid>
                                            <Var>TestOpDegs.Message_1</Var>
                                        </oid>
                                        <Rel>Message</Rel>
